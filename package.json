{
  "name": "colisee-injector",
  "version": "1.0.0",
  "description": "Micro-service responsible for scheduling random games for SIG-Game Colisee.",
  "main": "src/main.js",
  "scripts": {
    "quick-start": "node src/main.js",
    "prestart": "npm run build",
    "start": "npm run quick-start",
    "setup": "npm install",
    "build": "tsc",
    "quick-docs": "npm run quick-docs:ts && npm run quick-docs:api",
    "quick-docs:ts": "echo yo",
    "quick-docs:api": "apidoc --input src/ --output docs/docs/api/",
    "predocs": "npm run build",
    "docs": "npm run quick-docs",
    "quick-test": "mocha test/",
    "pretest": "npm run build",
    "test": "npm run quick-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siggame/colisee-injector.git"
  },
  "author": "siggame",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siggame/colisee-injector/issues"
  },
  "homepage": "https://github.com/siggame/colisee-injector#readme",
  "dependencies": {
    "chai": "^3.5.0",
    "express": "^4.14.1",
    "knex": "^0.12.6",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "pg": "^6.1.2"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/express": "^4.0.35",
    "@types/knex": "0.0.41",
    "@types/lodash": "^4.14.52",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "apidoc": "^0.17.5",
    "typedoc": "^0.5.5",
    "typescript": "^2.1.5"
  }
}
